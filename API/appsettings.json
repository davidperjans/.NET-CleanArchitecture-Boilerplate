{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "connection-string"
  },
  "JwtSettings": {
    "Issuer": "BoilerplateIssuer",
    "Audience": "BoilerplateAudience",
    "SecretKey": "SECRET-KEY",
    "ExpirationMinutes": 60
  },
  "AllowedHosts": "*"
}
